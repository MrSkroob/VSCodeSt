import json
from dependencies.project_loading_utils import load_project


project_json = load_project("scratch_project/Empty Scratch Project.sb3")


for i in project_json["targets"]:
    blocks = i["blocks"]
    for id in blocks:
        print(json.dumps(blocks[id], indent=4))


